{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "build": {
      "commands": {
        "build": "npx prisma generate && npm run build"
      }
    }
  }
  